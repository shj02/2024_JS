<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>array-push-pop</title>
</head>
<body>
    <script>
        const pets = ['개', '고양이', '앵무새'] // 문자열 const

        document.write(`[처음] --> ${pets.toString()} <br>`);
        pets.push(`햄스터`);
        document.write(`[push(후)] --> ${pets.toString()} <br>`);
        pets.pop();
        document.write(`[pop(후)] --> ${pets.toString()} <br>`);
    </script>
</body>
</html>
